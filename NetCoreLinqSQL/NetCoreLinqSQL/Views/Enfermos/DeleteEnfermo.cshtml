@model List<Enfermo> 


<h1>Eliminar Enfermo</h1>

<form method="post">

    <label>Inscripcion</label>
    <input type="text" name="inscripcion" class="form-control"/>
    <br/>

    <button type="submit" class="btn btn-secondary">Eliminar Enfermo</button>
</form>

@if (Model != null) {



<table class="table table-active">
        <thead class="table table-danger">
            <tr>
                <th>Inscripcion</th>
                <th>Apellido</th>
                <th>Direccion</th>
                <th>Fecha_nac</th>
                <th>NSS</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Enfermo enf in Model) { 

                <tr>
                    <td>@enf.Inscripcion</td>
                    <td>@enf.Apellido</td>
                </tr>
            }
        </tbody>
    </table>

}