@model List<Departamento>

<h1>Departamentos</h1>


<table class="table table-striped">
    <thead>
        <tr>
            <th>Número</th>
            <th>Nombre</th>
            <th>Localidad</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Departamento dep in Model) { 
            
            <tr>
                <td>@dep.IdDepartamento</td>
                <td>@dep.Nombre</td>
                <td>@dep.Localidad</td>
            </tr>
        }
    </tbody>
</table>