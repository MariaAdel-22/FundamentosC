@model List<Empleado>

@{


    List<string> cantidad = ViewBag.Cantidad as List<string>;
}

@if (Model != null)
{

    <h1>Resumen Compra</h1>

    <table class="table table-active">
        <thead>
            <tr>
                <th>Empleado</th>
                <th>Cantidad</th>
            </tr>
        </thead>
        <tbody>
            @for (int i = 0; i < Model.Count(); i++)
            {
                
                <tr>
                    <td>@Model[i].Apellido</td>
                    <td>@cantidad[i]</td>
                </tr>

            }

        </tbody>
    </table>

}
else { 
<h1>BUENADS</h1>
}